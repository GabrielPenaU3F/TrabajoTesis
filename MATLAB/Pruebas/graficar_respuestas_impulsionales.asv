function graficar_respuestas_impulsionales(h, fs)

    h = eliminar_distorsion_no_lineal(h);
    h_db = 20*real(log10(h./max(h)));
    h_hilbert = abs(hilbert(h));
    h_hilbert_suave = aplicar_filtro_media_movil(h_hilbert, 5001)
    ;
    e_t = 20*real(log10(h_hilbert./max(h_hilbert_suave)));
    t = 0:1/fs:length(h)/fs - 1/fs;
    plot(t,h_db,t,e_t);